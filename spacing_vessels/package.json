{
  "name": "spacing-vessels",
  "browserslist": [
    "> 0.5%",
    "not dead",
    "last 2 versions"
  ],
  "source": "./src/index.js",
  "scripts": {
    "start": "parcel",
    "clean": "rimraf ./dist",
    "build": "npm run lint && npm run clean && parcel build src/index.js --no-content-hash",
    "lint": "eslint  src/ --ext .js --ext .jsx",
    "lint-fix": "eslint src/ --ext .js --ext .jsx",
    "format": "prettier \"src/**/*.{js,jsx,html,css,scss}\"",
    "format-write": "prettier \"src/**/*.{js,jsx,html,css,scss}\" --write"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.3",
    "buffer": "^5.7.1",
    "classnames": "^2.3.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "parcel": "^2.8.3",
    "parcel-namer-without-hash": "^0.0.1",
    "prettier": "^2.8.7",
    "process": "^0.11.10"
  },
  "dependencies": {
    "@handsontable/react": "^12.3.3",
    "axios": "^1.3.5",
    "handsontable": "^12.3.3",
    "http-status-codes": "^2.2.0",
    "inputmask": "^5.0.8",
    "interactjs": "^1.10.17",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-calendar-timeline": "^0.28.0",
    "react-datepicker": "^4.11.0",
    "react-dom": "^18.2.0",
    "react-imask": "^6.6.1",
    "react-router": "^6.11.0",
    "react-router-dom": "^6.11.0",
    "react-select": "^5.7.3",
    "react-toastify": "^9.1.2",
    "uuid": "^9.0.0"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!(axios|react-toastify))"
  ]
}
